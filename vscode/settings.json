  "css.validate": false,
  "less.validate": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Victor Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": 700,
  "editor.formatOnSave": false,
  "editor.glyphMargin": false, // Removes margin for debugging indicators
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false, // Removes minimap
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false, // Removes border from overview ruler
  "editor.renderControlCharacters": false,
  "editor.renderIndentGuides": false, // Removes indent guides
  "editor.renderLineHighlight": "none", // Removes line highlight
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.suggest.shareSuggestSelections": false,
  "editor.suggestSelection": "first",
  "editor.trimAutoWhitespace": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "storage.type.js",
          "entity.name.function.js",
          "keyword.control.module.js",
          "entity.other.attribute-name.class.css",
          "variable.other.readwrite.js",
          "variable.other.readwrite.shorthandpropertyname.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 180,
  "emmet.extensionsPath": ["~"],
  "emmet.includeLanguages": {
    "html": "html",
    "css": "css",
    "javascript": "javascriptreact",
    "scss": "scss",
    "plaintext": "jade",
    "dockerfile": "dockerfile",
    "json": "json",
    "markdown": "html",
    "ruby": "ruby",
    "python": "python",
    "sql": "sql",
    "typescript": "typescriptreact",
    "yaml": "yaml",
    "erb": "html"
  },
  "emmet.excludeLanguages": [],
  "emmet.variables": {
    "lang": "en-US",
    "charset": "UTF-8"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "npm",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "explorer.openEditors.visible": 0, // Removes open editors section at the top of the sidebar.
  "files.associations": {
    "Dockerfile-prod": "dockerfile",
    "dev-docker-compose.yml": "docker-compose",
    "*.js": "javascript",
    ".functions": "shellscript",
    ".aliases": "shellscript",
    "*.dev": "dockerfile",
    "Dockerfile-dev": "dockerfile"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": false,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/errorScreenshots": true,
    "**/node_modules": false,
    "**/mock": false,
    "**/.github": false,
    "**/lib": false
  },
  "files.insertFinalNewline": false,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "html.autoClosingTags": true,
  "html.format.wrapAttributes": "force-aligned",
  "indentRainbow.colors": [
    "rgba(255, 162, 64, 0.2)",
    "rgba(255, 255, 255, 0.2)",
    "rgba(127, 255, 127, 0.2)"
  ],
  "indentRainbow.ignoreErrorLanguages": ["markdown"],
  "javascript.autoClosingTags": true,
  "javascript.suggest.autoImports": true,
  "javascript.suggest.completeJSDocs": true,
  "javascript.suggest.paths": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "markdown.preview.fontSize": 16,
  "ruby.rubocop.onSave": true,
  "scss.validate": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontFamily": "Victor Mono",
  "terminal.integrated.fontWeight": 700,
  "typescript.autoClosingTags": true,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.title": "${dirty} ${activeEditorShort}${separator}${rootName}",
  "workbench.editor.tabSizing": "shrink",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Hackpot Darker Knight",
  "yaml.schemas": {
    "file:///toc.schema.json": "/toc\\.yml/i"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.productIconTheme": "fluent-icons",
  "window.zoomLevel": -2,
  "workbench.iconTheme": "vscode-icons"
}
